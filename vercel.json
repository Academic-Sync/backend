{
    "version": 2,
    "builds": [
      {
        "src": "src/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "src/server.js"
      }
    ],
    "installCommand": "npm install",
    "developmentCommand": "npm run dev",
    "outputDirectory": "dist"
}